{
  "name": "MakerSquare LA Admin Chrome Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chrome Extension for MakerSquare LA Staff",
  "homepage_url": "http://makersquare.com",
  "content_security_policy": "script-src 'self' https://cdn.firebase.com https://*.firebaseio.com; object-src 'self'",
  "icons": {},
  "permissions":["storage"],
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/app.js"
    ],
    "persistent": true
  },
  "browser_action": {
    /*"default_icon": "icons/icon19.png",*/
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "css": [
        "styles.css"
      ]
    },
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "js/app.js"
      ]
    }
  ]
}
